@using OPM.SFS.Web.Models


@model List<AdminAgencyListViewModel.AgencyListItem>

<thead>
	<tr>
		<th>

		</th>
		<th>
			Agency Type
		</th>
		<th>
			Agency Name
		</th>
		<th>
			Sub-Agency Name
		</th>
	</tr>
</thead>
<tbody>
	@if (Model.Count() > 0)
	{
		@foreach (var item in Model)
		{
			<tr>
				<td>
					<a href="/Admin/AgencyEdit?aid=@item.AgencyID">Edit</a>
				</td>
				<td>
					@item.AgencyType
				</td>
				<td>
					@item.AgencyName
				</td>
				<td>
					@item.SubAgency
				</td>
			</tr>
		}
	}
</tbody>
